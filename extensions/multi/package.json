{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "multi",
  "title": "Multi",
  "description": "Raycast extension to prompt or open multiple links at once using a custom URL search query",
  "icon": "multi-icon.png",
  "author": "edg",
  "access": "public",
  "owner": "multi",
  "categories": [
    "Applications",
    "Communication",
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "preferences": [
    {
      "type": "dropdown",
      "title": "Target AppleScript Application",
      "required": true,
      "placeholder": "Multi",
      "name": "targetAppleScriptApplication",
      "description": "This extension uses AppleScript to control Multi. To target a specific build of Multi, adjust this preference as needed.",
      "default": "Multi",
      "data": [
        {
          "title": "Multi",
          "value": "Multi"
        },
        {
          "title": "Multi Alpha",
          "value": "Multi Alpha"
        },
        {
          "title": "Multi Dev",
          "value": "MultiDev"
        },
        {
          "title": "Multi Dev Personal",
          "value": "MultiDevPersonal"
        }
      ]
    }
  ],
  "commands": [
    {
      "name": "prompt",
      "title": "New Prompt",
      "subtitle": "Open multiple links at once using a custom URL search query",
      "keywords": [
        "new",
        "prompt",
        "query",
        "open"
      ],
      "description": "Open multiple links at once using a custom URL search query",
      "mode": "view"
    },
    {
      "name": "add-urls",
      "title": "Add Urls",
      "subtitle": "Add or remove links for Multi command",
      "keywords": [
        "add",
        "remove",
        "url",
        "links"
      ],
      "description": "Add or remove links for Multi command",
      "mode": "view"
    },
    {
      "name": "joinCallFromLink",
      "title": "Join Session Via Link",
      "subtitle": "Multi",
      "arguments": [
        {
          "name": "url",
          "placeholder": "https://call.multi.app/...",
          "required": true,
          "type": "text"
        }
      ],
      "keywords": [
        "https://call.multi.app",
        "https",
        "call",
        "multi",
        "link",
        "url",
        "join",
        "session"
      ],
      "description": "Join a Multi session from a given link.",
      "mode": "no-view"
    },
    {
      "name": "leaveSession",
      "title": "Leave Session",
      "subtitle": "Multi",
      "keywords": [
        "leave",
        "session",
        "call"
      ],
      "description": "Leave the current session.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.70.2",
    "@raycast/utils": "^1.13.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.11.30",
    "@types/react": "18.2.72",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "pull-contributions": "npx @raycast/api@latest pull-contributions"
  }
}
